{
  "name": "@ccusage/core",
  "type": "module",
  "version": "16.2.5",
  "description": "Shared utilities for ccusage",
  "author": "ryoppippi",
  "license": "MIT",
  "funding": "https://github.com/ryoppippi/ccusage?sponsor=1",
  "homepage": "https://github.com/ryoppippi/ccusage#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryoppippi/ccusage.git"
  },
  "bugs": {
    "url": "https://github.com/ryoppippi/ccusage/issues"
  },
  "exports": {
    ".": "./src/index.ts",
    "./consts": "./src/consts.ts",
    "./types": "./src/types.ts",
    "./date-utils": "./src/date-utils.ts",
    "./project-names": "./src/project-names.ts",
    "./session-blocks": "./src/session-blocks.ts",
    "./token-utils": "./src/token-utils.ts",
    "./json-output-types": "./src/json-output-types.ts",
    "./calculate-cost": "./src/calculate-cost.ts",
    "./pricing-fetcher": "./src/pricing-fetcher.ts",
    "./claude-code": "./src/claude-code/index.ts",
    "./claude-code-consts": "./src/claude-code-consts.ts",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=20.19.4"
  },
  "scripts": {
    "build": "tsdown",
    "format": "pnpm run lint --fix",
    "lint": "eslint --cache .",
    "prepack": "pnpm run build && clean-pkg-json",
    "prerelease": "pnpm /^prerelease:/",
    "prerelease:build": "pnpm run build",
    "prerelease:lint": "pnpm run lint",
    "prerelease:typecheck": "pnpm typecheck",
    "test": "TZ=UTC vitest --passWithNoTests",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "@antfu/utils": "catalog:runtime",
    "@ccusage/internal": "workspace:*",
    "@praha/byethrow": "catalog:runtime",
    "es-toolkit": "catalog:runtime",
    "fast-sort": "catalog:runtime",
    "path-type": "catalog:runtime",
    "tinyglobby": "catalog:runtime",
    "type-fest": "catalog:runtime",
    "valibot": "catalog:runtime",
    "xdg-basedir": "catalog:runtime"
  },
  "devDependencies": {
    "@oxc-project/runtime": "catalog:build",
    "fs-fixture": "catalog:testing",
    "publint": "catalog:lint",
    "tsdown": "catalog:build",
    "unplugin-unused": "catalog:build",
    "vitest": "catalog:testing"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./consts": "./dist/consts.js",
      "./types": "./dist/types.js",
      "./date-utils": "./dist/date-utils.js",
      "./project-names": "./dist/project-names.js",
      "./session-blocks": "./dist/session-blocks.js",
      "./token-utils": "./dist/token-utils.js",
      "./json-output-types": "./dist/json-output-types.js",
      "./calculate-cost": "./dist/calculate-cost.js",
      "./pricing-fetcher": "./dist/pricing-fetcher.js",
      "./claude-code": "./dist/claude-code/index.js",
      "./claude-code-consts": "./dist/claude-code-consts.js",
      "./package.json": "./package.json"
    }
  },
  "devEngines": {
    "runtime": [
      {
        "name": "node",
        "version": "^24.4.0",
        "onFail": "download"
      }
    ]
  }
}
