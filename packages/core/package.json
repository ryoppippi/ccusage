{
	"name": "@ccusage/core",
	"version": "16.2.5",
	"description": "Shared utilities for ccusage",
	"homepage": "https://github.com/ryoppippi/ccusage#readme",
	"bugs": {
		"url": "https://github.com/ryoppippi/ccusage/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ryoppippi/ccusage.git"
	},
	"funding": "https://github.com/ryoppippi/ccusage?sponsor=1",
	"license": "MIT",
	"author": "ryoppippi",
	"type": "module",
	"exports": {
		".": "./src/index.ts",
		"./table": "./src/table.ts",
		"./package.json": "./package.json"
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "tsdown",
		"format": "pnpm run lint --fix",
		"lint": "eslint --cache .",
		"prepack": "pnpm run build && clean-pkg-json",
		"prerelease": "pnpm /^prerelease:/",
		"prerelease:build": "pnpm run build",
		"prerelease:lint": "pnpm run lint",
		"prerelease:typecheck": "pnpm typecheck",
		"test": "TZ=UTC vitest --passWithNoTests",
		"typecheck": "tsgo --noEmit"
	},
	"dependencies": {
		"cli-table3": "catalog:runtime",
		"es-toolkit": "catalog:runtime",
		"picocolors": "catalog:runtime",
		"string-width": "catalog:runtime"
	},
	"devDependencies": {
		"publint": "catalog:lint",
		"tsdown": "catalog:build",
		"unplugin-unused": "catalog:build",
		"vitest": "catalog:testing"
	},
	"engines": {
		"node": ">=20.19.4"
	},
	"publishConfig": {
		"exports": {
			".": "./dist/index.js",
			"./table": "./dist/table.js",
			"./package.json": "./package.json"
		}
	},
	"devEngines": {
		"runtime": [
			{
				"name": "node",
				"version": "^24.4.0",
				"onFail": "download"
			}
		]
	}
}
